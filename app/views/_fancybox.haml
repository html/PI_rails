- content_for :head do
  = stylesheet_link_tag 'jquery.fancybox'
  = javascript_include_tag 'jquery/jquery.fancybox'

  :javascript
    $(function(){
      var images = [
        '/images/fancybox/fancy_progress.png',
        '/images/fancybox/fancy_closebox.png',
        '/images/fancybox/fancy_left.png',
        '/images/fancybox/fancy_right.png',
        '/images/fancybox/fancy_shadow_n.png',
        '/images/fancybox/fancy_shadow_ne.png',
        '/images/fancybox/fancy_shadow_e.png',
        '/images/fancybox/fancy_shadow_se.png',
        '/images/fancybox/fancy_shadow_s.png',
        '/images/fancybox/fancy_shadow_sw.png',
        '/images/fancybox/fancy_shadow_w.png',
        '/images/fancybox/fancy_shadow_nw.png',
        '/images/fancybox/fancy_title_left.png',
        '/images/fancybox/fancy_title_main.png',
        '/images/fancybox/fancy_title_right.png'
      ];

      for(var i=0;i< images.length;i++){
        (new Image()).src = images[i];
      }
    });
